{
    "name": "al-one/laravel-replace-into",
    "description": "SQL replace into for Laravel",
    "keywords": ["laravel", "database", "eloquent", "replace-into"],
    "authors": [
        {
            "name": "Alone",
            "email": "hi@anlo.ng"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/contracts": "^5.3 || ^6.0 || ^7.0 || ^8.0"
